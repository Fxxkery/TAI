{
  "$schema": "https://schema.tauri.app/config/2",
  "identifier": "com.example.temporal-agency-idle",
  "productName": "Temporal Repair Agency",
  "version": "0.5.2",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "dist",
    "devUrl": "http://localhost:5173"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "Temporal Repair Agency",
        "width": 1120,
        "height": 760,
        "resizable": true
      }
    ]
  },
  "bundle": {
    "active": true,
    "targets": [
      "msi",
      "nsis"
    ],
    "icon": [
      "icons/icon.ico"
    ]
  },
  "plugins": {}
}